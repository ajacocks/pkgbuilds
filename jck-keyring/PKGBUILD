pkgname=jck-keyring
pkgver=0.1
pkgrel=1
pkgdesc="Personal PGP Keyring"
arch=(any)
url="https://github.com/jckimble/pkgbuilds"
license=('ISC')
install="$pkgname.install"
source=('jck.gpg')
sha256sums=(SKIP)

pkgver() {
  printf "r%s.%s" "$(git rev-list --count HEAD)" "$(git rev-parse --short HEAD)"
}

package() {
  echo "Installing"
}